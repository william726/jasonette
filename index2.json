{
    "$jason": {
        "head": {
            "title": "Web Container Native Transition",
            "actions": {
                "$load": {
                    "type": "$set",
                    "options": {
                        "url": [{
                            "{{#if $params && 'url' in $params}}": "{{$params.url}}"
                        }, {
                            "{{#else}}": "https://github.com"
                        }]
                    },
                    "success": {
                        "type": "$render"
                    }
                },
                "navigate": {
                    "type": "$href",
                    "options": {
                        "url": "https://raw.githubusercontent.com/william726/jasonette/master/index2.json",
                        "options": {
                            "url": "{{$jason.url}}"
                        }
                    }
                }
            },
            "templates": {
                "body": {
                    "background": {
                        "type": "html",
                        "url": "{{$get.url}}",
                        "action": {
                            "trigger": "navigate"
                        }
                    }
                },
                "header": {
                    "title": "Test App",
                    "style": {
                      "background": "#ff0000",
                      "color": "#ffffff"
                }
          },
            }
        }
    }
}
